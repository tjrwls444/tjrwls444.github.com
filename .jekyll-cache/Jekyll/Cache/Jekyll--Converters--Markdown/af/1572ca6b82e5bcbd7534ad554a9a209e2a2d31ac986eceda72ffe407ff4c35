public class MystackLinked {
    // ì—°ê²° ë¦¬ìŠ¤íŠ¸ë¥¼ ìœ„í•œ Nodeí´ë˜ìŠ¤
    private class NodeClass { 
        Object data; 
        NodeClass link; 
        
        private NodeClass( Object item ) {
            this.data = item;
            this.link = null;
        }
    } 
    
    // ë§¨ ìœ„ì— ìˆëŠ” ë°ì´í„°ë¥¼ ìœ„í•œ ë³€ìˆ˜
    private NodeClass top;
    
    MystackLinked(){
        top = null;
    }
    // pop() ,  push() , peek() , isEmpty() 4ê°€ì§€ì˜ í•¨ìˆ˜ êµ¬í˜„
    public void push( Object item ) {
       // ì‹œê°„ ë³µì¡ë„ O(1)
       // ìƒˆë¡œìš´ ë…¸ë“œë¥¼ ë§Œë“¤ê³  ê¸°ì¡´ topì„ í•´ë‹¹ ë…¸ë“œì— ì—°ê²°
       NodeClass temp = new NodeClass(item);
       temp.link = top;
       // ì´ì œ ì‹ ê·œ topì€ ì§€ê¸ˆ ë§Œë“  ê²ƒì´ë‹¤.
       top = temp;
    }
    
    public Object pop() {
        // topì„ ë°”ê¿”ì£¼ê³  top ë°ì´í„°ë¥¼ ê°€ì§€ê³ ì˜´
        // ì‹œê°„ ë³µì¡ë„ O(1)
        if(isEmpty()) {
            throw new ArrayIndexOutOfBoundsException();
        } else {
            Object item = top.data;
            top = top.link;
            return item;
        }
     }
    
    public Object peek() {
        // top ë°ì´í„°ë§Œ ê°€ì§€ê³  ì˜´
        // ì‹œê°„ ë³µì¡ë„ O(1)
        if(isEmpty()) {
            throw new ArrayIndexOutOfBoundsException();
        } else {
            return top.data;
        }
     }
    
    public Boolean isEmpty() {
        // ì‹œê°„ ë³µì¡ë„ O(1)
        return top == null;
    }
}

** ì‚¬ìš©ë¶€ 
MystackLinked test = new MystackLinked();
test.push(1);
test.push(2);
test.push(3);
test.push("dd");
test.push("ee");

while(!test.isEmpty()) {
    System.out.println(test.pop());
}

