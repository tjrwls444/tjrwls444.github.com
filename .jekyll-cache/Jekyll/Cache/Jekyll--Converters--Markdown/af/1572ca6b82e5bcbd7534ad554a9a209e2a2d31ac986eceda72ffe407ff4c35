public class MystackArray {
    private int top;
    private int maxSize;
    private Object[] stackArray;
    
    // ë°°ì—´ì´ë‹¤ë³´ë‹ˆ í¬ê¸°ì§€ì •ì´ í•„ìš”í•˜ë‹¤.
    MystackArray(int size){
        this.top = -1;
        this.maxSize = size;
        this.stackArray = new Object[size];
    }
    // pop() ,  push() , peek() , isEmpty() 4ê°€ì§€ì˜ í•¨ìˆ˜ êµ¬í˜„
    public void push( Object item ) {
       //ì‹œê°„ ë³µì¡ë„ O(1)
       if(top &gt;= maxSize-1) throw new ArrayIndexOutOfBoundsException();
       stackArray[++top] = item;
    }
    
    public Object pop() {
        // topì„ ë°”ê¿”ì£¼ê³  top ë°ì´í„°ë¥¼ ê°€ì§€ê³ ì˜´
        //ì‹œê°„ ë³µì¡ë„ O(1)
        if(isEmpty()) {
            throw new ArrayIndexOutOfBoundsException();
        } else {
            return stackArray[top--];
        }
     }
    
    public Object peek() {
        // top ë°ì´í„°ë§Œ ê°€ì§€ê³  ì˜´
        //ì‹œê°„ ë³µì¡ë„ O(1)
        if(isEmpty()) {
            throw new ArrayIndexOutOfBoundsException();
        } else {
            return stackArray[top];
        }
     }
    
    public Boolean isEmpty() {
        //ì‹œê°„ ë³µì¡ë„ O(1)
        return top == -1;
    }
}

** ì‚¬ìš©ë¶€ 
MystackArray test2 = new MystackArray(5);
test2.push(1);
test2.push(2);
test2.push(3);
test2.push("dd");
test2.push("ee");

while(!test2.isEmpty()) {
    System.out.println(test2.pop());
}

