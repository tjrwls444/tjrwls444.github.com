public class Main {
    static int N, K, Mid, Len;
    static long SUM;    
    static int Tree[], Data[];
    static void update(int i, int n)
    {
        int idx = Len + n; // lowë°ì´í„° ì‹œì‘ìœ„ì¹˜ í™•ì¸ : indexìì²´ë¥¼ ê°’ìœ¼ë¡œ ì‘ìš©í•˜ê¸° ìœ„í•´ ë“¤ì–´ì˜¨ ìˆ˜ ë§Œí¼ + n ì‹œí‚¨ ìœ„ì¹˜ì˜ ê°’ì„ 1ë¡œ ë³€ê²½
        
        while(idx &gt; 0)
        {
            Tree[idx] += i; // í•´ë‹¹ ì„¤ê³„ë¥¼ ê°€ì§€ê³  ì¸ë±ìŠ¤ íŠ¸ë¦¬ ë§Œë“¤ê¸°
            idx /= 2;
        }
    }
    static int search(int k)
    {
        int i = 1;
        int l = 0;
        while(i &lt; (1&lt;&lt;16))
        {
            //tree ë£¨íŠ¸ì¸ index 1ë¶€í„° ê²€ìƒ‰ (ë§¨ ì²˜ìŒì—ëŠ” Kê°¯ìˆ˜ ë§Œí¼ ì €ì¥ë˜ì–´ìˆì„ ê²ƒì„ &gt;&gt; ì´ê²ƒì„ midì¸ (K+1)/2 ìœ„ì¹˜ì˜ ìˆ«ìë¥¼ ì°¾ì•„ê°€ëŠ” ê³¼ì •)
            l = Tree[i*2]; 
            // ìì‹ë…¸ë“œ í™•ì¸ ìì‹ë…¸ë“œì— ì €ì¥ëœ ê°¯ìˆ˜ í™•ì¸
            
            // ìì‹ë…¸ë“œì— ì €ì¥ëœ ìˆ˜ê°€  ( ex) 5 ) ì¤‘ê°„ê°’ (ex ) 2 )ë³´ë‹¤ í¬ê±°ë‚˜ ê°™ë‹¤ë©´ ê³„ì† ìì‹ íƒìƒ‰
            if(k &lt;= l)
            {
                i = i * 2; // ìì‹ ë…¸ë“œì˜ ì™¼ìª½ ê°’
            }
            else
            {
                // ì‘ì•„ë²„ë¦¬ë©´  ex) k = 2 , ã…£ = 1
                // ë§ˆì§€ë§‰ê°’ì„ ì°¾ê¸°ìœ„í•´  kë¥¼ ì¤„ì´ê³ 
                k = k - l;
                // ìì‹ì˜ ì˜¤ë¥¸ìª½ ë…¸ë“œë¥¼ íƒìƒ‰í•œë‹¤.
                i = i * 2 + 1; //ìì‹ ë…¸ë“œì˜ ì˜¤ë¥¸ìª½ ê°’
            }
        }

        // ìµœì¢… indexì˜ ìœ„ì¹˜ì¸ iì—ì„œ ì‹œì‘ì ì„ ë¹¼ë©´ ì¤‘ê°„ ê°’ì„ ì¶”ì¶œí•  ìˆ˜ ìˆìŒ
        return i - Len;
    }
    public static void main(String[] args) throws NumberFormatException, IOException {
        Scanner sc = new Scanner(System.in);
        
        N = sc.nextInt();
        K = sc.nextInt();
        Mid = (K+1)/2;
        Len = 1&lt;&lt;16; // íŠ¸ë¦¬ì˜ ë¡œìš°ë°ì´í„° ì‹œì‘ìœ„ì¹˜ëŠ” Nì˜ ìµœëŒ€í¬ê¸°ì¸ 2ì˜16ìŠ¹ = 65536
        Tree = new int[2*Len+1];
        Data = new int[N+1];
        for(int i = 1 ; i &lt;= N ; i++)
        {
            Data[i] = sc.nextInt();
            if(i &lt; K)
            {
                update(1, Data[i]);
                // kê°€ ë„˜ì–´ê°€ê¸° ì „ê¹Œì§„ ë°ì´í„°ë¥¼ íŠ¸ë¦¬ì— ë„£ê¸°ë§Œ í•¨.
            }
            else 
            {
                if(i &gt; K)
                {
                    update(-1, Data[i - K]);
                    // ë§Œì•½ Kê°€ ë„˜ì–´ê°€ëŠ” ìˆ˜ëŠ” ë°”ë¡œ ì•ì— ìˆ˜ë¥¼ ì œê±°í•´ ì£¼ì–´ì•¼ í•œë‹¤.
                    
                }               
                update(1, Data[i]);
                SUM += search(Mid);
                // ì¤‘ì•™ê°’ ì°¾ê¸°
            }
        }
        System.out.println(SUM);
        sc.close();     
    }
}


