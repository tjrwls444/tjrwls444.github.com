public class MyQueueLinked {
    // ì—°ê²° ë¦¬ìŠ¤íŠ¸ë¥¼ ìœ„í•œ Nodeí´ë˜ìŠ¤
    private class NodeClass { 
        Object data; 
        NodeClass link; 
        
        private NodeClass( Object item ) {
            this.data = item;
            this.link = null;
        }
    } 
    
    private NodeClass top;  // ë§¨ ìœ„ì— ìˆëŠ” ë°ì´í„°ë¥¼ ìœ„í•œ ë³€ìˆ˜
    private NodeClass tail; // ê¼¬ë¦¬ì— ìˆëŠ” ë°ì´í„°ë¥¼ ìœ„í•œ ë³€ìˆ˜
    
    MyQueueLinked(){
        top  = null;
        tail = null;
    }
    
    //poll(),  add() , peek() , isEmpty() 4ê°€ì§€ì˜ í•¨ìˆ˜ êµ¬í˜„
    public void add( Object item ) {
       // ìƒˆë¡œìš´ ë…¸ë“œë¥¼ ë§Œë“¤ê³  ê¸°ì¡´ tailì„ í•´ë‹¹ìœ¼ë¡œ êµì²´í•˜ë©´ì„œ ê¸°ì¡´ tail.linkë¥¼ ì‹ ê·œ ë…¸ë“œë¡œ ë³€ê²½
       // ì²« ì§„ì… ì‹œ topì´ë‘ í…Œì¼ì€ ê°™ë‹¤.
       // ì‹œê°„ ë³µì¡ë„ O(1)
       NodeClass old = tail;
       // ì´ì œ ì‹ ê·œë¡œ ë§Œë“  ê²ƒë“¤ì€ tailì— ì§‘ì–´ë„£ê¸°
       tail = new NodeClass(item);;
       if( isEmpty() ) {
           top = tail;
       } else {
           old.link = tail;
       }
    }
    
    public Object poll() {
        // topì„ íƒ‘ì— ì—°ê²°ëœ linkë¡œ ë°”ê¿”ì£¼ê³  topë°ì´í„° ê°€ì§€ê³  ì˜¤ê¸°
        // ì‹œê°„ ë³µì¡ë„ O(1)
        if(isEmpty()) {
            throw new ArrayIndexOutOfBoundsException();
        } else {
            Object item = top.data;
            top = top.link;
            return item;
        }
     }
    
    public Object peek() {
        // top ë°ì´í„°ë§Œ ê°€ì§€ê³  ì˜´
        // ì‹œê°„ ë³µì¡ë„ O(1)
        if(isEmpty()) {
            throw new ArrayIndexOutOfBoundsException();
        } else {
            return top.data;
        }
     }
    
    public Boolean isEmpty() {
        // ì‹œê°„ ë³µì¡ë„ O(1)
        return top == null;
    }
}


** ì‚¬ìš©ë¶€ 
MyQueueLinked test = new MyQueueLinked();
test.add(1);
test.add(2);
test.add(3);
test.add("dd");
test.add("ee");

while(!test.isEmpty()) {
    System.out.println(test.poll());
}

