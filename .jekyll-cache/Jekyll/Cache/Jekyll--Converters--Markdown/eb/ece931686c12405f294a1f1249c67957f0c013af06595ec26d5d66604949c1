public class MyQueueArray {
    private int top; // ë§¨ ìœ„ì— ìˆëŠ” ë°ì´í„°ë¥¼ ìœ„í•œ ë³€ìˆ˜
    private int tail; // ê¼¬ë¦¬ì— ìˆëŠ” ë°ì´í„°ë¥¼ ìœ„í•œ ë³€ìˆ˜
    private int maxSize;
    private Object[] stackArray;
    
    // ë°°ì—´ì´ë‹¤ë³´ë‹ˆ í¬ê¸°ì§€ì •ì´ í•„ìš”í•˜ë‹¤.
    MyQueueArray(int size){
        this.top = -1;
        this.tail = -1;
        this.maxSize = size;
        this.stackArray = new Object[size];
    }
    //poll(),  add() , peek() , isEmpty() 4ê°€ì§€ì˜ í•¨ìˆ˜ êµ¬í˜„
    public void add( Object item ) {
       //ì‹œê°„ ë³µì¡ë„ O(1)
       if(tail &gt;= maxSize-1) throw new ArrayIndexOutOfBoundsException();
       stackArray[++tail] = item;
    }
    
    public Object poll() {
        // topì„ ë°”ê¿”ì£¼ê³  top ë°ì´í„°ë¥¼ ê°€ì§€ê³ ì˜´
        //ì‹œê°„ ë³µì¡ë„ O(1)
        if(isEmpty()) {
            throw new ArrayIndexOutOfBoundsException();
        } else {
            return stackArray[++top];
        }
     }
    
    public Object peek() {
        // top ë°ì´í„°ë§Œ ê°€ì§€ê³  ì˜´
        //ì‹œê°„ ë³µì¡ë„ O(1)
        if(isEmpty()) {
            throw new ArrayIndexOutOfBoundsException();
        } else {
            return stackArray[top+1];
        }
     }
    
    public Boolean isEmpty() {
        //ì‹œê°„ ë³µì¡ë„ O(1)
        if( top == tail ) {
            // ì´ˆê¸°í™” ë¡œì§
            top  = -1;
            tail = -1;
        }
        return top == tail;
    }
}


** ì‚¬ìš©ë¶€ 
MyQueueArray test2 = new MyQueueArray(5);
test2.add(1);
test2.add(2);
test2.add(3);
test2.add("dd");
test2.add("ee");

while(!test2.isEmpty()) {
    System.out.println(test2.poll());
    
}

